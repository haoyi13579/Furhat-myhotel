  		<item>yeah</item>
  		<item>yes</item>
  		<item>okay</item>
  		<item>OK</item>
  		<item>of course</item>
  	</one-of>
  	<tag>out.yes=1</tag>
  </rule>
  
  <rule id="no" scope="public">
  	<one-of>
  		<item>no</item>
  		<item>nope</item>
  		<item>not</item>
  		<item>nothing</item>
  	</one-of>
  	<tag>out.no=1</tag>
  </rule>
  
  <rule id="exit" scope="public">
  	<one-of>
  		<item>exit</item>
  		<item>restart</item>
  	</one-of>
  	<tag>out.exit=1</tag>
  </rule>
  
</grammar>
