<?xml version="1.0" encoding="utf-8"?>
<grammar xml:lang="en-US" version="1.0" 
	xmlns="http://www.w3.org/2001/06/grammar" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.w3.org/2001/06/grammar http://www.iristk.net/xml/srgs.xsd" tag-format="semantics/1.0">
	
  <rule id="languagesetting" scope="public">
  	<one-of>
  		<item>
  			<one-of>
  				<item>English</item>
  				<item>英语</item>
  				<item>英文</item>
  			</one-of>
  			<tag>out.english=1</tag>
  		</item>
  		<item>
  			<one-of>
  				<item>Chinese</item>
  				<item>Mandarin</item>
  				<item>中文</item>
  				<item>普通话</item>
  			</one-of>
  			<tag>out.chinese=1</tag>
  		</item>	
  	</one-of>
    </rule>


  <rule id="attraction" scope="public">
  	<one-of>
		<item>attraction_</item>
		<item>tourist attraction_</item>
		<item>place_</item>
		<item>view_</item>  	
  	</one-of>
  	<tag>out.attractions=1</tag>
  </rule>
  
  <rule id="natural" scope="public">
  	<one-of>
  		<item>natural</item>
  	</one-of>
  	<tag>out.natural=1</tag>
  </rule>
  
  <rule id="historical" scope="public">
  	<one-of>
  		<item>historical</item>
  	</one-of>
  	<tag>out.historical=1</tag>
  </rule>
  
  <rule id="popular" scope="public">
  	<one-of>
  		<item>popular</item>
  	</one-of>
  	<tag>out.popular=1</tag>
  </rule>
  
  <rule id="attrname" scope="public">
  	<one-of>
		<item>
			<one-of>
				<item>Glasgow Botanic Garden_</item>
				<item>Garden_</item>
				<item>Botanic Garden_</item>
			</one-of>
			<tag>out.attrname="Glasgow Botanic Gardens"</tag>
		</item>
		<item>
			<one-of>
				<item>Pollok Country Park</item>
				<item>Country Park</item>
				<item>Country</item>
			</one-of>
			<tag>out.attrname="Pollok Country Park"</tag>
		</item>
		<item>
			<one-of>
				<item>Kelvingrove Park</item>
				<item>Park</item>
			</one-of>
			<tag>out.attrname="Kelvingrove Park"</tag>
		</item>
		<item>
			<one-of>
				<item>Kelvingrove Art Gallery and Museum</item>
				<item>Art</item>
				<item>Gallery</item>
				<item>Art Gallery</item>
				<item>Art Gallery and Museum</item>
			</one-of>
			<tag>out.attrname="Kelvingrove Art Gallery and Museum"</tag>
		</item>
		<item>
			<one-of>
				<item>Riverside Museum</item>
				<item>Riverside</item>
			</one-of>
			<tag>out.attrname="Riverside Museum"</tag>
		</item>
		<item>
			<one-of>
				<item>Glasgow Cathedral</item>
				<item>Cathedral</item>
			</one-of>
			<tag>out.attrname="Glasgow Cathedral"</tag>
		</item>
  	</one-of>
  </rule>
  
  <rule id="transportation" scope="public">
  	<one-of>
  		<item>transportation</item>
  		<item>bus</item>
  		<item>subway</item>
  	</one-of>
  	<tag>out.transportation=1</tag>
  </rule>
  
  
  
  <rule id="room" scope="public">
  	<one-of>
  		<item>room_</item>
  		<item>live</item>
  	</one-of>
  	<tag>out.rooms=1</tag>
  </rule>
  

  
  <rule id="otherroom" scope="public">
  	<one-of>
  		<item>restaurant_</item>
  		<item>kitchen</item>
  		<item>dining hall</item>
  		<item>eat</item> 
  		<item>gym</item> 
  		<item>sport_</item>		
  	</one-of>
  		<tag>out.otherroom=1</tag> 
  </rule>
  
  <rule id="weather" scope="public">
  	<item>weather</item>
  	<tag>out.weather=1</tag>
  </rule>
  
  <rule id="morningcall" scope="public">
  	<one-of>
  		<item>
  			<one-of>
  				<item>call</item>
  				<item>morning</item>
  				<item>morning call</item>
  				<item>awake</item>
  				<item>wake</item>
  			</one-of>
  			<tag>out.call=1</tag>
  		</item>
  	</one-of>
  </rule>
  
  <rule id="clock" scope="public">
  	<one-of>
  		<item>o'clock</item>
  		<item>clock</item>
  	</one-of>
  	<tag>out.clock=1</tag>
  </rule>
  
  <rule id="half" scope="public">
  	<one-of>
  		<item>half</item>
  	</one-of>
  	<tag>out.half=1</tag>
  </rule>
  
