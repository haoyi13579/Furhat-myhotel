  <rule id="date" scope="public">
  	<one-of>
  		<item>tomorrow<tag>out.date= "tomorrow"</tag></item>
  		<item>today<tag>out.date= "today"</tag></item>
  	</one-of>
  </rule>
  
  <rule id="apm" scope="public">
  	<one-of>
  		<item>
  			<one-of>
  				<item>morning</item>
  				<item>am</item>
  				<item>a.m.</item>
  				<item>AM</item>
  				<item>A.M.</item>
  			</one-of>
  			<tag>out.apm="a.m."</tag>
  		</item>
  		<item>
  			<one-of>
  				<item>afternoon</item>
  				<item>evening</item>
  				<item>pm</item>
  				<item>p.m.</item>
  				<item>PM</item>
  				<item>P.M.</item>
  			</one-of>
  			<tag>out.apm="p.m."</tag>
  		</item>
  	</one-of>
  </rule>
  
  <rule id="othertime" scope="public">
  	<one-of>
  		<item>midnight<tag>out.midnight=1</tag></item>
  		<item>noon<tag>out.noon=1</tag></item>
  	</one-of>
  </rule>
  
  <rule id="past_to" scope="public">
  	<one-of>
  		<item>past<tag>out.past="past"</tag></item>
  		<item>to<tag>out.to="to"</tag></item>
  	</one-of>
  </rule>
  
  
	
	
  
